              "kind": "method",
              "name": "extractHeaders",
              "parameters": [
                {
                  "name": "headers"
                }
              ],
              "description": "Extract headers from various formats"
            },
            {
              "kind": "method",
              "name": "extractResponse",
              "parameters": [
                {
                  "name": "response"
                }
              ],
              "description": "Extract response data"

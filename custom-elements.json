            {
              "kind": "method",
              "name": "removeListener",
              "parameters": [
                {
                  "name": "callback",
                  "description": "Callback to remove",
                  "type": {
                    "text": "ExchangeCallback"
                  }
                }
              ],
              "description": "Remove a listener"
            },
            {
              "kind": "method",
              "name": "notifyListeners",
              "parameters": [
                {
                  "name": "exchange",
                  "description": "HTTP exchange to send to listeners",
                  "type": {
                    "text": "Object"
                  }
                }
              ],
              "description": "Notify all listeners with captured data\nApplies filter and pause checks before notifying",
              "privacy": "private"
            },
            {
              "kind": "method",
              "name": "matchesFilter",
              "parameters": [
                {
                  "name": "url"
                }
              ],
              "description": "Check if URL matches filter pattern"
